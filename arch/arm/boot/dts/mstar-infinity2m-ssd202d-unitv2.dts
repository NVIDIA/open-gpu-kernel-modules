/dts-v1/;
#include "mstar-infinity2m-ssd202d.dtsi"

/ {
	model = "UnitV2";
	compatible = "m5stack,unitv2", "mstar,infinity2m";

	aliases {
		serial0 = &pm_uart;
	};

	chosen {
		stdout-path = "serial0:115200n8";
	};
};

&pm_uart {
	status = "okay";
};
