#include "omap3xxx-clocks.dtsi"

/* Preferred always-on timer for clockevent. Some boards must use dmtimer12 */
&timer1_target {
	ti,no-reset-on-init;
	ti,no-idle;
	timer@0 {
		assigned-clocks = <&gpt1_fck>;
		assigned-clock-parents = <&omap_32k_fck>;
	};
};
