			ti,syss-mask = <1>;
			clocks = <&gpt1_fck>, <&gpt1_ick>;
			clock-names = "fck", "ick";
			#address-cells = <1>;
			#size-cells = <1>;
			ranges = <0x0 0x48318000 0x1000>;

			timer1: timer@0 {
				compatible = "ti,omap3430-timer";
				reg = <0x0 0x80>;
				clocks = <&gpt1_fck>;
				clock-names = "fck";
				interrupts = <37>;
