			clock-names = "apb", "hosc", "losc";
			gpio-controller;
			interrupt-controller;
			#interrupt-cells = <3>;
			#gpio-cells = <3>;

			can0_ph_pins: can0-ph-pins {
				pins = "PH20", "PH21";
