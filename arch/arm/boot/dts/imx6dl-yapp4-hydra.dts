	status = "okay";
};

&oled_1309 {
	status = "okay";
};

&pcie {
	status = "okay";
};

&reg_pcie {
	status = "okay";
};

&touchkeys {
	status = "okay";
};

&usdhc3 {
	status = "okay";
};
