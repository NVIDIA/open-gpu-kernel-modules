			opp-supported-hw = <0x000F>;
		};

		opp@54000000 {
			opp-microvolt = <950000 950000 1300000>;
			opp-hz = /bits/ 64 <54000000>;
			opp-supported-hw = <0x000F>;
		};

		opp@57000000 {
			opp-microvolt = <950000 950000 1300000>;
			opp-hz = /bits/ 64 <57000000>;
			opp-supported-hw = <0x000F>;
		};

		opp@100000000 {
			opp-microvolt = <1000000 1000000 1300000>;
			opp-hz = /bits/ 64 <100000000>;
			opp-supported-hw = <0x000F>;
		};

		opp@108000000 {
