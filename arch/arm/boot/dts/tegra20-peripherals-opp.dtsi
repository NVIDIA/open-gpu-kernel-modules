/ {
	emc_icc_dvfs_opp_table: emc-dvfs-opp-table {
		compatible = "operating-points-v2";

		opp@36000000 {
			opp-microvolt = <950000 950000 1300000>;
			opp-hz = /bits/ 64 <36000000>;
			opp-supported-hw = <0x000F>;
		};

		opp@47500000 {
			opp-microvolt = <950000 950000 1300000>;
			opp-hz = /bits/ 64 <47500000>;
			opp-supported-hw = <0x000F>;
		};

