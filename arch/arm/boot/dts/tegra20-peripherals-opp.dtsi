		};

		opp@600000000 {
			opp-microvolt = <1200000 1200000 1300000>;
			opp-hz = /bits/ 64 <600000000>;
			opp-supported-hw = <0x000F>;
		};

		opp@666000000 {
			opp-microvolt = <1200000 1200000 1300000>;
			opp-hz = /bits/ 64 <666000000>;
			opp-supported-hw = <0x000F>;
		};

		opp@760000000 {
			opp-microvolt = <1300000 1300000 1300000>;
			opp-hz = /bits/ 64 <760000000>;
