/dts-v1/;

#include "bcm3384_viper.dtsi"

/ {
	compatible = "brcm,bcm93384wvg-viper", "brcm,bcm3384-viper";
	model = "Broadcom BCM93384WVG-viper";

	chosen {
		bootargs = "console=ttyS0,115200";
		stdout-path = &uart0;
	};
};

&uart0 {
	status = "okay";
};

&ehci0 {
	status = "okay";
};

&ohci0 {
	status = "okay";
};
