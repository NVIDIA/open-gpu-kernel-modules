config SOC_AR5312
	bool "Atheros AR5312/AR2312+ SoC support"
	depends on ATH25
	default y

config SOC_AR2315
	bool "Atheros AR2315+ SoC support"
	depends on ATH25
	default y

config PCI_AR2315
	bool "Atheros AR2315 PCI controller support"
	depends on SOC_AR2315
	select ARCH_HAS_PHYS_TO_DMA
	select FORCE_PCI
	default y
