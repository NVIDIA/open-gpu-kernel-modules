#include "stingray.dtsi"
#include <dt-bindings/gpio/gpio.h>

/ {
	aliases {
		serial0 = &uart1;
		serial1 = &uart0;
		serial2 = &uart2;
		serial3 = &uart3;
	};

	chosen {
		stdout-path = "serial0:115200n8";
	};
};

&memory { /* Default DRAM banks */
	reg = <0x00000000 0x80000000 0x0 0x80000000>, /* 2G @ 2G */
	      <0x00000008 0x80000000 0x1 0x80000000>; /* 6G @ 34G */
};

&enet {
	phy-mode = "rgmii-id";
	phy-handle = <&gphy0>;
};

&uart1 {
	status = "okay";
};

&sdio0 {
	non-removable;
	full-pwr-cycle;
};

&sdio1 {
	full-pwr-cycle;
};

&mdio_mux_iproc {
	mdio@10 {
		gphy0: eth-phy@10 {
			reg = <0x10>;
		};
	};
};
