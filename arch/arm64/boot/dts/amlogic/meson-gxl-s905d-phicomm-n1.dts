/dts-v1/;

#include "meson-gxl-s905d-p230.dts"

/ {
	compatible = "phicomm,n1", "amlogic,s905d", "amlogic,meson-gxl";
	model = "Phicomm N1";

	cvbs-connector {
		status = "disabled";
	};

	leds {
		compatible = "gpio-leds";

		status {
			label = "n1:white:status";
			gpios = <&gpio_ao GPIOAO_9 GPIO_ACTIVE_HIGH>;
			default-state = "on";
		};
	};
};

&cvbs_vdac_port {
	status = "disabled";
};

&usb {
	dr_mode = "host";
};
