/include/ "p1010si-pre.dtsi"

/ {
	model = "fsl,P1010RDB-PB";
	compatible = "fsl,P1010RDB-PB";

	/include/ "p1010rdb_32b.dtsi"
};

/include/ "p1010rdb.dtsi"

&phy0 {
	interrupts = <0 1 0 0>;
};

&phy1 {
	interrupts = <2 1 0 0>;
};

&phy2 {
	interrupts = <1 1 0 0>;
};

/include/ "p1010si-post.dtsi"
