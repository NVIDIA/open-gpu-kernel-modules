obj-$(CONFIG_PARIDE_ON20)	+= on20.o
obj-$(CONFIG_PARIDE_ON26)	+= on26.o
obj-$(CONFIG_PARIDE_KTTI)	+= ktti.o
obj-$(CONFIG_PARIDE_BPCK6)	+= bpck6.o
obj-$(CONFIG_PARIDE_PD)		+= pd.o
